    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.3",
    "express-winston": "^4.2.0",
    "fov": "^1.1.1",
    "i18n": "^0.15.1",
    "jimp": "^0.22.12",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "multer": "^1.4.5-lts.1",
